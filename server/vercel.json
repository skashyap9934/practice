{
  "version": 2,
  "builds": [{ "src": "./index.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
      "origin": "*",
      "credentials": true,
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
